# TradingView Authentication
SESSION=your_tradingview_session_cookie
SIGNATURE=your_tradingview_signature_cookie

# Environment
NODE_ENV=development
LOG_LEVEL=debug

# API Configuration
API_PORT=3000
API_HOST=localhost

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/gecko_indicator
REDIS_URL=redis://localhost:6379

# Model Configuration
MODEL_PATH=./data/models/gecko_model.json
CHECKPOINT_PATH=./data/models/checkpoints

# Data Collection
DATA_SYMBOLS=BTCUSDT,ETHUSDT,EURUSD,GBPUSD,SPY
DEFAULT_TIMEFRAME=5m

# Trading Parameters
MIN_PATTERN_CONFIDENCE=0.70
MIN_BACKTEST_SHARPE_RATIO=1.5
MIN_BACKTEST_WIN_RATE=0.65

# Backtesting
BACKTEST_START_DATE=2024-01-01
BACKTEST_END_DATE=2025-10-31
BACKTEST_INITIAL_CAPITAL=10000

# Alert Configuration
ENABLE_ALERTS=true
ALERT_WEBHOOK_URL=
SLACK_WEBHOOK_URL=

# Feature Engineering
ATR_LENGTH=14
EMA_LENGTHS=8,21,50,200
NORMALIZE_METHOD=minmax
