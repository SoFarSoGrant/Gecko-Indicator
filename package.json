{
  "name": "gecko-ml-indicator",
  "version": "0.1.0",
  "description": "Multi-timeframe ML-based Gecko trading indicator using TradingView-API",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "collect:data": "node scripts/collect-data.js",
    "train:model": "node scripts/train-model.cjs",
    "backtest": "node scripts/backtest.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "validate:env": "node scripts/validate-env.js"
  },
  "keywords": [
    "trading",
    "machine-learning",
    "gecko-pattern",
    "tradingview",
    "multi-timeframe",
    "technical-analysis"
  ],
  "author": "Trading Project Team",
  "license": "MIT",
  "dependencies": {
    "@mathieuc/tradingview": "^3.5.2",
    "@tensorflow/tfjs": "^4.11.0",
    "@tensorflow/tfjs-node": "^4.11.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "winston": "^3.11.0",
    "cors": "^2.8.5",
    "joi": "^17.11.0"
  },
  "optionalDependencies": {
    "redis": "^4.6.11",
    "pg": "^8.11.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "eslint": "^8.53.0",
    "prettier": "^3.1.0",
    "@types/node": "^20.10.0",
    "mocha": "^10.2.0",
    "chai": "^4.3.10"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "data/*",
      "tests/*",
      "*.test.js"
    ],
    "delay": 500
  }
}
